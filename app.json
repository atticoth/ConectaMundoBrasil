{
  "expo": {
    "name": "Conecta Mundo",
    "slug": "conectamundo",
    "version": "4.8.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconeapp.png",
    "userInterfaceStyle": "light",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medeirosge.conectamundo",
      "buildNumber": "18",
      "infoPlist": {
        "LSMinimumSystemVersion": "12.0",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconeapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.medeirosge.conectamundo",
      "versionCode": 5
    },
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/images/iconeapp.png"
    },
    "extra": {
      "eas": {
        "projectId": "e20d9bed-97e5-430e-b35e-6358d2e9f279"
      }
    },
    "owner": "medeirosge",
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "18.0"
          }
        }
      ]
    ]
  }
}
